{
    "name": "yoelpc4/laravel-cloudinary",
    "description": "Laravel Cloudinary filesystem cloud driver.",
    "keywords": [
        "laravel",
        "cloudinary",
        "filesystem",
        "cloud"
    ],
    "homepage": "https://github.com/yoelpc4/laravel-cloudinary",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoel Peter Chandra",
            "email": "yoelpc4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cloudinary/cloudinary_php": "^2.9.0",
        "illuminate/contracts": "11.*",
        "illuminate/filesystem": "11.*",
        "illuminate/http": "11.*",
        "illuminate/support": "11.*",
        "league/flysystem": "^3.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "9.*",
        "phpunit/phpunit": "11.*"
    },
    "autoload": {
        "psr-4": {
            "Yoelpc4\\LaravelCloudinary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoelpc4\\LaravelCloudinary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yoelpc4\\LaravelCloudinary\\CloudinaryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
